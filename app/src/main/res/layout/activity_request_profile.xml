<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFDF"
    tools:context=".MyPage.RequestProfile.RequestProfileActivity">

    <TextView
        android:id="@+id/textView39"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        android:text="회원정보 변경"
        android:textColor="#F09661"
        android:textSize="30sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:text="아이디"
        android:textSize="14sp"
        app:layout_constraintBottom_toBottomOf="@+id/re_userid"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/re_userid" />

    <TextView
        android:id="@+id/re_userid"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="80dp"
        android:background="#56DADADA"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintStart_toEndOf="@+id/textView40"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView43"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:background="#56DADADA"
        android:gravity="center_vertical"
        android:text="수정불가"
        android:textAlignment="center"
        android:textColor="#99FF0000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/re_userid"
        app:layout_constraintStart_toEndOf="@+id/re_userid"
        app:layout_constraintTop_toTopOf="@+id/re_userid" />

    <TextView
        android:id="@+id/textView42"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이메일"
        app:layout_constraintBottom_toBottomOf="@+id/re_email"
        app:layout_constraintEnd_toEndOf="@+id/textView40"
        app:layout_constraintStart_toStartOf="@+id/textView40"
        app:layout_constraintTop_toTopOf="@+id/re_email" />

    <EditText
        android:id="@+id/re_email"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        app:layout_constraintEnd_toEndOf="@+id/textView43"
        app:layout_constraintStart_toStartOf="@+id/re_userid"
        app:layout_constraintTop_toBottomOf="@+id/re_userid" />

    <TextView
        android:id="@+id/textView44"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="비밀번호"
        app:layout_constraintBottom_toBottomOf="@+id/re_pass"
        app:layout_constraintEnd_toEndOf="@+id/textView42"
        app:layout_constraintStart_toStartOf="@+id/textView42"
        app:layout_constraintTop_toTopOf="@+id/re_pass" />

    <EditText
        android:id="@+id/re_pass"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/re_email"
        app:layout_constraintStart_toStartOf="@+id/re_email"
        app:layout_constraintTop_toBottomOf="@+id/re_email" />

    <TextView
        android:id="@+id/textView46"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="재확인"
        app:layout_constraintBottom_toBottomOf="@+id/re_pass2"
        app:layout_constraintEnd_toEndOf="@+id/textView44"
        app:layout_constraintStart_toStartOf="@+id/textView44"
        app:layout_constraintTop_toTopOf="@+id/re_pass2" />

    <EditText
        android:id="@+id/re_pass2"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintEnd_toEndOf="@+id/re_pass"
        app:layout_constraintStart_toStartOf="@+id/re_pass"
        app:layout_constraintTop_toBottomOf="@+id/re_pass" />

    <TextView
        android:id="@+id/textView47"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="이 름"
        app:layout_constraintBottom_toBottomOf="@+id/re_username"
        app:layout_constraintEnd_toEndOf="@+id/textView46"
        app:layout_constraintStart_toStartOf="@+id/textView46"
        app:layout_constraintTop_toTopOf="@+id/re_username" />

    <TextView
        android:id="@+id/re_username"
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:background="#56DADADA"
        android:gravity="center_vertical"
        android:text="TextView"
        android:textSize="18sp"
        app:layout_constraintStart_toStartOf="@+id/re_pass2"
        app:layout_constraintTop_toBottomOf="@+id/re_pass2" />

    <TextView
        android:id="@+id/textView49"
        android:layout_width="80dp"
        android:layout_height="40dp"
        android:background="#56DADADA"
        android:gravity="center_vertical"
        android:text="수정불가"
        android:textAlignment="center"
        android:textColor="#99FF0000"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/re_username"
        app:layout_constraintEnd_toEndOf="@+id/re_pass2"
        app:layout_constraintStart_toEndOf="@+id/re_username"
        app:layout_constraintTop_toTopOf="@+id/re_username" />

    <TextView
        android:id="@+id/textView51"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="닉네임"
        app:layout_constraintBottom_toBottomOf="@+id/re_subname"
        app:layout_constraintEnd_toEndOf="@+id/textView47"
        app:layout_constraintStart_toStartOf="@+id/textView47"
        app:layout_constraintTop_toTopOf="@+id/re_subname" />

    <EditText
        android:id="@+id/re_subname"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="@+id/textView49"
        app:layout_constraintStart_toStartOf="@+id/re_username"
        app:layout_constraintTop_toBottomOf="@+id/re_username" />

    <TextView
        android:id="@+id/textView50"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="전화번호"
        app:layout_constraintBottom_toBottomOf="@+id/re_num"
        app:layout_constraintEnd_toEndOf="@+id/textView47"
        app:layout_constraintStart_toStartOf="@+id/textView47"
        app:layout_constraintTop_toTopOf="@+id/re_num" />

    <EditText
        android:id="@+id/re_num"
        android:layout_width="280dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:ems="10"
        android:inputType="phone"
        app:layout_constraintEnd_toEndOf="@+id/re_subname"
        app:layout_constraintStart_toStartOf="@+id/re_subname"
        app:layout_constraintTop_toBottomOf="@+id/re_subname" />

    <Button
        android:id="@+id/re_fin"
        android:layout_width="155dp"
        android:layout_height="60dp"
        android:layout_marginTop="32dp"
        android:background="#F09661"
        android:text="변경하기"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="@+id/textView50"
        app:layout_constraintTop_toBottomOf="@+id/re_num" />

    <Button
        android:id="@+id/re_close"
        android:layout_width="155dp"
        android:layout_height="60dp"
        android:layout_marginStart="32dp"
        android:background="#F09661"
        android:text="취소하기"
        android:textColor="#FFFFFF"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="@+id/re_fin"
        app:layout_constraintEnd_toEndOf="@+id/re_num"
        app:layout_constraintStart_toEndOf="@+id/re_fin"
        app:layout_constraintTop_toTopOf="@+id/re_fin" />

</androidx.constraintlayout.widget.ConstraintLayout>